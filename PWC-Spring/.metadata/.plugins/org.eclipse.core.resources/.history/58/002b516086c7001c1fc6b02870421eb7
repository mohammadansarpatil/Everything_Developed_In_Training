package com.lambda;

public class Employee {
	
	private int statusCode;
	private int id;
	private String name;
	private String city;
	private String email;
	public int getStatusCode() {
		return statusCode;
	}
	public void setStatusCode(int statusCode) {
		this.statusCode = statusCode;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getCity() {
		return city;
	}
	public void setCity(String city) {
		this.city = city;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	@Override
	public String toString() {
		return "Employee [statusCode=" + statusCode + ", id=" + id + ", name=" + name + ", city=" + city + ", email="
				+ email + "]";
	}
	public Employee() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Employee(int statusCode, int id, String name, String city, String email) {
		super();
		this.statusCode = statusCode;
		this.id = id;
		this.name = name;
		this.city = city;
		this.email = email;
	}
	
	
	

}
